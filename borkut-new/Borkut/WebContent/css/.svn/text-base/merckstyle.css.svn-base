/*BEGIN style for SUCCESS messages*/
.suc_lt { background:url(../images/org_curve_leftTop.gif) no-repeat left top;  position:relative }
.suc_rt { background:url(../images/org_curve_rightTop.gif) no-repeat right top;}
/*.suc_t { background:url(../images/org_h_topLine.GIF) repeat-x;}
.suc_b { background:url(../images/org_h_bottomLine.gif) repeat-x;}*/
.suc_tb { border-bottom: 1px solid #ff9900; border-top: 1px solid #ff9900; margin-left:5px; margin-right: 5px;}
/*.suc_l { background:url(../images/org_v_leftLine.gif) repeat-y ;}
.suc_r { background:url(../images/org_v_rightLine.gif) repeat-y ;}*/
.suc_lr{ border-left: 1px solid #ff9900; border-right: 1px solid #ff9900; margin-top: 3px; margin-bottom:3px;  margin-right:-5px;  margin-left:-5px; padding:5px; }
.suc_ld { background:url(../images/org_curve_leftBottom.gif) no-repeat left bottom;  }
.suc_rd { background:url(../images/org_curve_rightBottom.gif) no-repeat right bottom;}
.suc_con { background:url(../images/suc_icon1.gif) no-repeat left; font-family:Verdana, Arial, Helvetica, sans-serif; font-size: 10pt; font-weight: bold; color: #FCAF17; line-height:18px; padding:5px 0px 5px 30px; }
#success_com td {padding-bottom:10px;}
/*END style for SUCCESS messages*/


/*BEGIN style for ALERT messages*/
.fail_lt { background:url(../images/red_curve_leftTop.gif) no-repeat left top;  position:relative }
.fail_rt { background:url(../images/red_curve_rightTop.gif) no-repeat right top;}
.fail_tb { border-bottom: 1px solid #ff0044; border-top: 1px solid #ff0044; margin-left:5px; margin-right:5px;}
.fail_lr{ border-left:1px solid #ff0044; border-right:1px solid #ff0044; margin-right:-5px;  margin-left:-5px;  padding:5px; margin-top:3px; margin-bottom:3px;}
.fail_ld { background:url(../images/red_curve_leftBottom.gif) no-repeat left bottom;  }
.fail_rd { background:url(../images/red_curve_rightBottom.gif) no-repeat right bottom;}
.fail_con { background:url(../images/alert_icon.gif) no-repeat left; font-family:Verdana, Arial, Helvetica, sans-serif; font-size: 10pt; font-weight: bold; color: #de0106; line-height:18px; padding:5px 0px 5px 30px; }
#fail_com td {padding-bottom:10px;}
/*END style for ALERT messages*/